{
	"compilerOptions": {
		"baseUrl": "./server",
		"paths": {
			"#data/*": ["data/*"],
			"#config/*": ["config/*"],
			"#models/*":["models/*"],
			"#routes/*":["routes/*"],
			"#middlewares/*":["middlewares/*"],
			"#controllers/*":["controllers/*"],
			"#utils/*":["utils/*"],
		},
		"module": "CommonJS",
		"target": "ES6"
	},
	"include": ["server/**/*"],
	"exclude": ["node_modules", "**/node_modules/*"]
}
